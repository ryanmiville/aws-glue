{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "aws-glue",
  "title": "AWS Glue",
  "description": "Find, open, and run AWS Glue jobs.",
  "icon": "command-icon.png",
  "author": "ryanmiville",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Glue Jobs",
      "description": "Find and open a glue job (or run it)",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@aws-sdk/client-glue": "^3.515.0",
    "@raycast/api": "^1.67.1",
    "@raycast/utils": "^1.12.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
